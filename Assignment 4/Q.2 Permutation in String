class Solution {

    public boolean checkInclusion(String s1, String s2) {
        if(s2.length()<s1.length()){
            return false;
        }
       int freq[]=new int[26];
       int len=s1.length();
       for(int i=0;i<len;i++){
        freq[s1.charAt(i)-'a']++;
       }
       int arr[]=new int[26];
       for(int i=0;i<len;i++){
        arr[s2.charAt(i)-'a']++;
       }
       if(Arrays.equals(freq, arr)){
        return true;
       }
        for(int i=len;i<s2.length();i++){
            arr[s2.charAt(i-len)-'a']--;
            arr[s2.charAt(i)-'a']++;
            if(Arrays.equals(freq, arr)){
                return true;
            }
        }
        return false;
       

    }
}