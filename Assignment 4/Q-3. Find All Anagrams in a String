class Solution {
    public List<Integer> findAnagrams(String s2, String s1) {
        if(s2.length()<s1.length()){
            return new ArrayList<Integer>();
        }
        List<Integer> set=new ArrayList<>();
      
       int freq[]=new int[26];
       int len=s1.length();
       for(int i=0;i<len;i++){
        freq[s1.charAt(i)-'a']++;
       }
       int arr[]=new int[26];
       for(int i=0;i<len;i++){
        arr[s2.charAt(i)-'a']++;
       }
       if(Arrays.equals(freq, arr)){
            set.add(0);
       }
       
        for(int i=len;i<s2.length();i++){
            arr[s2.charAt(i-len)-'a']--;
            arr[s2.charAt(i)-'a']++;
            if(Arrays.equals(freq, arr)){
                set.add(i-len+1);
            }
        }
        return set;
    }
}