class Solution {
    public int rob(int[] nums) {
        int dp[]=new int[nums.length];
        return sol(nums,nums.length-1,dp);
    }
    public static int sol(int[] nums,int n,int[] dp){
        if(n<1){
            return nums[n];
        }
        dp[0]=nums[0];
        dp[1]=Math.max(nums[0],nums[1]);
        for(int i=2;i<=n;i++){
            dp[i]=Math.max(dp[i-1],dp[i-2]+nums[i]);
        }
        return dp[n];
    }
}