class Solution {
    public int uniquePaths(int m, int n) {
        int arr[][]=new int[2][n+1];
        arr[0][n-1]=1;
        int t=m;
        while(m>0){
            for(int i=n-1;i>=0;i--){
                if(t==m && i==n-1){
                    t--;
                    continue;
                }
                arr[0][i]=arr[0][i+1]+arr[1][i];
            }
            for(int i=0;i<n;i++){
                arr[1][i]=arr[0][i];
            }
            m--;
        }
        return arr[0][0];
    }
}

